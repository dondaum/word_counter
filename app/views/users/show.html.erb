<div class="container">
  <div class="row">
    <div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1">
      <ul class="list-group">
        <li class="list-group-item">
          <h1> <%= @user.username %> </h1>
          <p> E-Mail: <%= @user.email %> <p>
          <p> Registriert am: <%= @user.created_at %> <p>
          <p> Mitglied seid: <%= member_since %> Tagen <p>
          <p> <%= link_to "Delete User", @user, method: :delete, data: {confirm: "Are you sure?"} %><p>
          </li>
        </ul>
      </div>
    </div>
  </div>
