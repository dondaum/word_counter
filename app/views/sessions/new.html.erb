<div class="container">
  <div class="row">
    <div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1">
      <div class="jumbotron">
        Login
      </div>
      <%= form_tag '/login' do %>
        <ul>Email: <%= text_field_tag :email %></ul>
        <ul>Passwort: <%= password_field_tag :password %></ul>
        <ul><%= submit_tag "Login" %></ul>
      <% end %>
    </div>
  </div>
</div>
